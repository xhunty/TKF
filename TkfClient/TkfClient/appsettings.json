{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "System.*": "Warning"
    }
  },
  "ConnectionStrings": {
    "db": ""
  },
  "AccessToken": "",
  "NLog": {
    "internalLogFile": "${basedir}/logs/internal.log",
    "internalLogLevel": "Info",
    "targets": {
      "async": true,
      "fileTarget": {
        "type": "File",
        "fileName": "${basedir}/logs/log-${shortdate}.log",
        "layout": "${longdate}|${uppercase:${level}}|${logger}|${message}|${exception:format=tostring}"
      }
    },
    "rules": [
      {
        "logger": "TkfClient.*",
        "minLevel": "Info",
        "writeTo": "fileTarget"
      }
    ]
  }
}